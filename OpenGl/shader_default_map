	s_light(vec4(0, 1, 0, 0), vec3(-6, 0, -10)),
	s_light(vec4(0, 0, 1, 0), vec3(0, 6, -10))
);

#define GEONUM	1
s_geo	geos[] = s_geo[](
	s_geo(SPHERE, vec3(0), 2, vec4(0), vec4(0), vec4(0), vec4(0), ms[0])
);
