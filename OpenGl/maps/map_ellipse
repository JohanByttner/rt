s_mat ms[] = s_mat[](
		s_mat(vec4(1, 0, 0, 1), 0.6, 0.6, 0, vec4(0), vec2(0), vec4(0))
);

#define LINUM	2

s_light lights[] = s_light[](
		s_light(vec4(1), vec3(6, 0, -10)),
		s_light(vec4(1), vec3(0, 0, 10))
);

#define GEONUM	2

s_geo geos[] = s_geo[](
	s_geo(ELLIPSE, vec3(0, 0, 0), 0, vec4(1, 4, 1, 2), vec4(0), vec4(0), vec4(0), ms[0]),
	s_geo(PLANE, vec3(0, 0, 20), 0, vec4(0, 0, 1, 0), vec4(0), vec4(0), vec4(0), ms[0])
);
